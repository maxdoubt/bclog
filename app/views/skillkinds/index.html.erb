<h1>Listing skillkinds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skillkinds.each do |skillkind| %>
      <tr>
        <td><%= skillkind.name %></td>
        <td><%= link_to 'Show', skillkind %></td>
        <td><%= link_to 'Edit', edit_skillkind_path(skillkind) %></td>
        <td><%= link_to 'Destroy', skillkind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Skillkind', new_skillkind_path %>
